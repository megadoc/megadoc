    "url": "https://github.com/tinydoc/tinydoc/issues"
  },
  "homepage": "https://github.com/tinydoc/tinydoc",
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "classnames": "^2.1.5",
    "commander": "^2.8.1",
